{
    "name": "Pic.me-server",
    "version": "1.0.0",
    "description": "pic.me API",
    "main": "index.js",
    "repository": "https://github.com/Pic-me-Pic-me/Pic.me-server",
    "author": "<dragontaek98@naver.com>",
    "license": "MIT",
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development nodemon",
        "deploy:dev": "cross-env NODE_ENV=development pm2 start dist",
        "start:prod": "cross-env NODE_ENV=production nodemon",
        "deploy:prod": "cross-env NODE_ENV=production pm2 start dist",
        "build": "tsc",
        "db-pull:dev": "dotenv -e .env.development npx prisma db pull",
        "db-push:dev": "dotenv -e .env.development npx prisma db push",
        "generate:dev": "dotenv -e .env.development npx prisma generate",
        "db-pull:prod": "dotenv -e .env.production npx prisma db pull",
        "db-push:prod": "dotenv -e .env.production npx prisma db push",
        "generate:prod": "dotenv -e .env.production npx prisma generate",
        "test:dev": "yarn mocha test/**/*.ts -r ts-node/register --exit"
    },
    "dependencies": {
        "@prisma/client": "^4.8.0",
        "@types/cors": "^2.8.13",
        "aws-sdk": "^2.1286.0",
        "axios": "^1.2.2",
        "bcryptjs": "^2.4.3",
        "cookie": "^0.5.0",
        "cors": "^2.8.5",
        "cross-env": "^7.0.3",
        "crypto-js": "^4.1.1",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "dotenv-cli": "^7.0.0",
        "express": "^4.18.2",
        "express-validator": "^6.14.2",
        "jsonwebtoken": "^9.0.0",
        "multer": "^1.4.5-lts.1",
        "multer-s3": "^3.0.1",
        "prisma": "^4.8.0",
        "typescript": "^4.9.4",
        "web-push": "^3.6.2"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/chai": "^4.3.4",
        "@types/cookie": "^0.5.1",
        "@types/crypto-js": "^4.1.1",
        "@types/express": "^4.17.15",
        "@types/express-validator": "^3.0.0",
        "@types/jsonwebtoken": "^9.0.0",
        "@types/mocha": "^10.0.1",
        "@types/multer": "^1.4.7",
        "@types/multer-s3": "^3.0.0",
        "@types/node": "^18.11.18",
        "@types/supertest": "^2.0.12",
        "@types/web-push": "^3.3.2",
        "eslint": "^8.31.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-airbnb-typescript": "^17.0.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-prettier": "^4.2.1",
        "husky": "^8.0.0",
        "nodemon": "^2.0.20",
        "prettier": "^2.8.1",
        "supertest": "^6.3.3",
        "ts-node": "^10.9.1"
    }
}
